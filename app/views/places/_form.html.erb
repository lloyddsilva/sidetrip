<%= simple_form_for(@place) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :description %>
    <%= f.input :photo_url %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :category %>
    <%= f.input :visit_time_minutes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
